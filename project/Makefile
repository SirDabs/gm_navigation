# GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=server
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),server)
  navigation_config = server
endif
ifeq ($(config),client)
  navigation_config = client
endif

PROJECTS := navigation

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

navigation:
ifneq (,$(navigation_config))
	@echo "==== Building navigation ($(navigation_config)) ===="
	@${MAKE} --no-print-directory -C . -f navigation.make config=$(navigation_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f navigation.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  server"
	@echo "  client"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   navigation"
	@echo ""
	@echo "For more information, see http://industriousone.com/premake/quick-start"